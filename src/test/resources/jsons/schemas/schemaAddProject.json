{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$ref": "#/definitions/AddProject",
  "definitions": {
    "AddProject": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "hourlyRate": {
          "$ref": "#/definitions/HourlyRate"
        },
        "clientId": {
          "type": "string"
        },
        "workspaceId": {
          "type": "string"
        },
        "billable": {
          "type": "boolean"
        },
        "memberships": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Membership"
          }
        },
        "color": {
          "type": ["string", "null"]
        },
        "estimate": {
          "$ref": "#/definitions/Estimate"
        },
        "archived": {
          "type": "boolean"
        },
        "duration": {
          "type": "string"
        },
        "clientName": {
          "type": "string"
        },
        "note": {
          "type": "string"
        },
        "costRate": {
          "type": "null"
        },
        "timeEstimate": {
          "$ref": "#/definitions/TimeEstimate"
        },
        "budgetEstimate": {
          "type": "null"
        },
        "estimateReset": {
          "type": "null"
        },
        "public": {
          "type": "boolean"
        },
        "template": {
          "type": "boolean"
        }
      },
      "required": [
        "archived",
        "billable",
        "budgetEstimate",
        "clientId",
        "clientName",
        "color",
        "costRate",
        "duration",
        "estimate",
        "estimateReset",
        "hourlyRate",
        "id",
        "memberships",
        "name",
        "note",
        "public",
        "template",
        "timeEstimate",
        "workspaceId"
      ],
      "title": "AddProject"
    },
    "Estimate": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "estimate": {
          "type": "string"
        },
        "type": {
          "type": "string"
        }
      },
      "required": [
        "estimate",
        "type"
      ],
      "title": "Estimate"
    },
    "HourlyRate": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "amount": {
          "type": "integer"
        },
        "currency": {
          "type": "string"
        }
      },
      "required": [
        "amount",
        "currency"
      ],
      "title": "HourlyRate"
    },
    "Membership": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "userId": {
          "type": "string"
        },
        "hourlyRate": {
          "type": "null"
        },
        "costRate": {
          "type": "null"
        },
        "targetId": {
          "type": "string"
        },
        "membershipType": {
          "type": "string"
        },
        "membershipStatus": {
          "type": "string"
        }
      },
      "required": [
        "costRate",
        "hourlyRate",
        "membershipStatus",
        "membershipType",
        "targetId",
        "userId"
      ],
      "title": "Membership"
    },
    "TimeEstimate": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "estimate": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "resetOption": {
          "type": "null"
        },
        "active": {
          "type": "boolean"
        },
        "includeNonBillable": {
          "type": "boolean"
        }
      },
      "required": [
        "active",
        "estimate",
        "includeNonBillable",
        "resetOption",
        "type"
      ],
      "title": "TimeEstimate"
    }
  }
}